{
  "server": {
    "port": 4040
  },
  "mongodb": {
    "feixunDB": {
      "dbUrl": "mongodb://localhost:27017/",
      "dbUser": "",
      "dbPassword": "",
      "dbName": "adFeixun",
      "dbCollection": {
        "device": "device",
        "online": "online"
      }
    }
  },
  "logs": {
    "appenders": {
      "errorLogger": {
        "type": "dateFile",
        "filename": "./logs/err/err",
        "pattern": "-yyyy-MM-dd.log",
        "alwaysIncludePattern": true
      },
      "resLogger": {
        "type": "dateFile",
        "filename": "./logs/log/log",
        "pattern": "-yyyy-MM-dd.log",
        "alwaysIncludePattern": true
      }
    },
    "categories": {
      "err": {
        "appenders": [
          "errorLogger"
        ],
        "level": "error"
      },
      "default": {
        "appenders": [
          "resLogger"
        ],
        "level": "info"
      }
    }
  },
  "transmit": {
    "routerLive": {
      "transmitUrl": "http://c.so9.cc:8082/routerlive/feixun/",
      "//intervalTime": "单位：分钟",
      "intervalTime": 10
    }
  }
}
